enable
configure terminal
hostname RT-RJ
enable secret enable@sec4u
banner motd "ACESSO SOMENTE PESSOAS AUTORIZADAS!"
ip domain name sec4u.local
crypto key generate rsa general-keys modulus 1024
username root privilege 15 secret root@sec4u
line vty 0 15 
password ssh@sec4u
transport input ssh
login local
exit
line console 0
password console@sec4u
exit
service password-encryption
no ip domain-lookup
int s0/0/0
ip address 200.200.2.2 255.255.255.248
no shutdown
int s0/0/1
ip address 200.200.3.2 255.255.255.248
no shutdown
int g0/0
shutdown
int g0/1
no shutdown
int g0/1.10
encapsulation dot1q 10
ip address 192.168.1.10 255.255.255.240
int g0/1.20
encapsulation dot1q 20
ip address 192.168.2.10 255.255.255.240
int g0/1.30
encapsulation dot1q 30
ip address 192.168.3.10 255.255.255.240
int g0/1.40
encapsulation dot1q 40
ip address 192.168.4.10 255.255.255.240
ip route 0.0.0.0 0.0.0.0 200.200.2.1 
ip route 0.0.0.0 0.0.0.0 200.200.3.1
exit
copy running-config startup-config 
